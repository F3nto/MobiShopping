{
  "expo": {
    "name": "Mobi Shop",
    "slug": "MobiShopping",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appImg2.png",
    "splash": {
      "image": "./assets/appImg1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package" : "com.mobishop",
      "versionCode" : 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/appImg2.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
